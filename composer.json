{
    "name": "twint/twint",
    "description": "TBU - Twint payment plugin",
    "type": "shopware-platform-plugin",
    "version": "0.0.1",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "twint-ch/twint-sdk": "dev-develop",
        "chillerlan/php-qrcode": "4.3.4",
        "ext-openssl": "*",
        "symfony/browser-kit": "7.1.x-dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://git.nfq.asia/twint-ag/php-sdk.git"
        }
    ],
    "authors": [
        {
            "name": "TWINT"
        },
        {
            "name": "NFQ"
        }
    ],
    "extra": {
        "shopware-plugin-class": "Twint\\TwintPayment",
        "copyright": "(c) by Twint 2024 (c) by NFQ",
        "label": {
            "de-DE": "TBU - Twint Payment",
            "en-GB": "TBU - Twint Payment",
            "fr-FR": "TBU - Twint Payment"
        },
        "description": {
            "de-DE": "TBU - Twint Payment",
            "en-GB": "TBU - Twint Payment"
        },
        "supportLink": {
            "de-DE": "https://localhost/",
            "en-GB": "https://localhost/"
        }
    },
    "autoload": {
        "psr-4": {
            "Twint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Twint\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "2.0.x-dev",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
