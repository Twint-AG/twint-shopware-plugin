<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Twint\Subscriber\CheckoutConfirmPageSubscriber">
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Twint\Subscriber\SystemConfigSubscriber">
            <argument type="service" id="Twint\Core\Service\SettingServiceInterface"/>
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Twint\ExpressCheckout\Subscriber\ProductPageLoadedSubscriber">
            <argument type="service" id="Twint\ExpressCheckout\Service\ExpressCheckoutButtonService"/>
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Twint\Subscriber\PaymentDistinguishableNameSubscriber">
            <argument type="service" id="translator"/>
            <tag name="kernel.event_subscriber"/>
        </service>

    </services>
</container>
